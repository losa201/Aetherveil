# Aetherveil Sentinel Configuration
# Copy this file to .env and customize for your environment

# Application Environment
ENVIRONMENT=development
DEBUG=true
TESTING=false

# Application Info
APP_NAME=Aetherveil Sentinel
APP_VERSION=1.0.0
APP_DESCRIPTION=Advanced Penetration Testing AI Agent

# Database Configuration
# SQLite
DATABASE__SQLITE_PATH=aetherveil.db
DATABASE__SQLITE_POOL_SIZE=20
DATABASE__SQLITE_TIMEOUT=30

# Neo4j
DATABASE__NEO4J_URI=bolt://localhost:7687
DATABASE__NEO4J_USER=neo4j
DATABASE__NEO4J_PASSWORD=your_neo4j_password_here
DATABASE__NEO4J_DATABASE=neo4j
DATABASE__NEO4J_POOL_SIZE=50
DATABASE__NEO4J_TIMEOUT=30

# Redis
DATABASE__REDIS_HOST=localhost
DATABASE__REDIS_PORT=6379
DATABASE__REDIS_DB=0
DATABASE__REDIS_PASSWORD=your_redis_password_here
DATABASE__REDIS_POOL_SIZE=10
DATABASE__REDIS_TIMEOUT=5

# Security Configuration
SECURITY__ENCRYPTION_KEY=your_256_bit_encryption_key_here_must_be_32_chars_minimum
SECURITY__JWT_SECRET=your_jwt_secret_key_here_must_be_32_chars_minimum
SECURITY__JWT_ALGORITHM=HS256
SECURITY__JWT_EXPIRATION_HOURS=24

# API Security
SECURITY__API_KEY_LENGTH=32
SECURITY__API_RATE_LIMIT=100
SECURITY__API_BURST_LIMIT=200

# Password Security
SECURITY__PASSWORD_MIN_LENGTH=12
SECURITY__PASSWORD_REQUIRE_SPECIAL=true
SECURITY__PASSWORD_REQUIRE_NUMBERS=true
SECURITY__PASSWORD_REQUIRE_UPPERCASE=true
SECURITY__PASSWORD_HASH_ROUNDS=12

# Session Security
SECURITY__SESSION_TIMEOUT_MINUTES=60
SECURITY__MAX_FAILED_ATTEMPTS=5
SECURITY__LOCKOUT_DURATION_MINUTES=15

# TLS/SSL
SECURITY__TLS_ENABLED=true
SECURITY__TLS_CERT_PATH=/path/to/cert.pem
SECURITY__TLS_KEY_PATH=/path/to/key.pem
SECURITY__TLS_CA_PATH=/path/to/ca.pem

# Network Configuration
NETWORK__COORDINATOR_HOST=0.0.0.0
NETWORK__COORDINATOR_PORT=8000
NETWORK__COORDINATOR_WORKERS=4

# Agent Communication
NETWORK__AGENT_HEARTBEAT_INTERVAL=30
NETWORK__AGENT_TIMEOUT=120

# ZMQ Configuration
NETWORK__ZMQ_PORT=5555
NETWORK__ZMQ_HWM=1000
NETWORK__ZMQ_LINGER=1000

# gRPC Configuration
NETWORK__GRPC_PORT=50051
NETWORK__GRPC_MAX_WORKERS=10
NETWORK__GRPC_MAX_MESSAGE_SIZE=4194304

# Load Balancer
NETWORK__LOAD_BALANCER_ENABLED=false
NETWORK__LOAD_BALANCER_ALGORITHM=round_robin
NETWORK__HEALTH_CHECK_INTERVAL=30

# Connection Limits
NETWORK__MAX_CONNECTIONS=1000
NETWORK__CONNECTION_TIMEOUT=30
NETWORK__KEEP_ALIVE_TIMEOUT=5

# GCP Configuration
GCP__PROJECT_ID=tidy-computing-465909-i3
GCP__REGION=europe-west1
GCP__ZONE=europe-west1-b

# Storage
GCP__STORAGE_BUCKET=aetherveil-storage
GCP__STORAGE_CLASS=STANDARD
GCP__BACKUP_BUCKET=aetherveil-backups

# Secret Manager
GCP__SECRET_MANAGER_PREFIX=aetherveil
GCP__SECRET_MANAGER_ENABLED=true

# Compute Engine
GCP__COMPUTE_MACHINE_TYPE=e2-standard-4
GCP__COMPUTE_DISK_SIZE=100
GCP__COMPUTE_DISK_TYPE=pd-standard

# Kubernetes
GCP__GKE_CLUSTER_NAME=aetherveil-cluster
GCP__GKE_NODE_COUNT=3
GCP__GKE_NODE_MACHINE_TYPE=e2-standard-2

# Cloud Functions
GCP__CLOUD_FUNCTIONS_REGION=europe-west1
GCP__CLOUD_FUNCTIONS_MEMORY=512
GCP__CLOUD_FUNCTIONS_TIMEOUT=300

# Monitoring
GCP__MONITORING_ENABLED=true
GCP__LOGGING_ENABLED=true
GCP__ALERTING_ENABLED=true

# Service Account
GCP__SERVICE_ACCOUNT_PATH=/path/to/service-account.json

# Agent Configuration
AGENT__MAX_AGENTS=100
AGENT__AGENT_SPAWN_RATE=5
AGENT__AGENT_LIFECYCLE_TIMEOUT=3600

# Resource Limits
AGENT__MAX_CPU_PER_AGENT=0.5
AGENT__MAX_MEMORY_PER_AGENT=512
AGENT__MAX_DISK_PER_AGENT=1024

# Communication
AGENT__MESSAGE_QUEUE_SIZE=1000
AGENT__HEARTBEAT_INTERVAL=30
AGENT__DISCOVERY_INTERVAL=60

# Persistence
AGENT__STATE_PERSISTENCE_ENABLED=true
AGENT__CHECKPOINT_INTERVAL=300

# Scaling
AGENT__AUTO_SCALING_ENABLED=true
AGENT__SCALE_UP_THRESHOLD=0.8
AGENT__SCALE_DOWN_THRESHOLD=0.3

# Security
AGENT__AGENT_ISOLATION_ENABLED=true
AGENT__SANDBOX_ENABLED=true

# Reinforcement Learning Configuration
RL__ALGORITHM=PPO
RL__POLICY_TYPE=MlpPolicy
RL__LEARNING_RATE=0.0003
RL__BATCH_SIZE=64
RL__BUFFER_SIZE=100000

# PPO Parameters
RL__GAMMA=0.99
RL__EPSILON=0.2
RL__ENTROPY_COEF=0.01
RL__VALUE_COEF=0.5

# Training
RL__TOTAL_TIMESTEPS=1000000
RL__EVAL_FREQ=10000
RL__SAVE_FREQ=50000

# Environment
RL__ENV_NAME=AetherVeilEnv
RL__NUM_ENVS=4

# Checkpointing
RL__CHECKPOINT_DIR=./checkpoints
RL__MODEL_SAVE_PATH=./models

# Hyperparameter Optimization
RL__HYPEROPT_ENABLED=false
RL__HYPEROPT_TRIALS=50

# Stealth Configuration
STEALTH__PROXY_ROTATION=true
STEALTH__PROXY_TIMEOUT=30
STEALTH__PROXY_RETRIES=3

# Traffic Shaping
STEALTH__JITTER_MIN=0.5
STEALTH__JITTER_MAX=2.0
STEALTH__REQUEST_DELAY=1.0

# Tor Configuration
STEALTH__TOR_ENABLED=false
STEALTH__TOR_PROXY=socks5://127.0.0.1:9050
STEALTH__TOR_CONTROL_PORT=9051

# Fingerprint Evasion
STEALTH__FINGERPRINT_EVASION=true
STEALTH__RANDOMIZE_HEADERS=true
STEALTH__SPOOF_TIMEZONE=true

# Rate Limiting
STEALTH__MAX_REQUESTS_PER_MINUTE=30
STEALTH__BURST_PROTECTION=true

# Logging Configuration
LOGGING__LOG_LEVEL=INFO
LOGGING__LOG_FILE=aetherveil.log
LOGGING__LOG_FILE_MAX_SIZE=10485760
LOGGING__LOG_FILE_BACKUP_COUNT=5

# Console Logging
LOGGING__CONSOLE_LOGGING=true
LOGGING__CONSOLE_LOG_LEVEL=INFO

# Structured Logging
LOGGING__JSON_LOGGING=true
LOGGING__INCLUDE_TRACE=false

# Security Logging
LOGGING__SECURITY_LOG_FILE=security.log
LOGGING__AUDIT_LOG_FILE=audit.log

# Performance Logging
LOGGING__PERFORMANCE_LOGGING=true
LOGGING__SLOW_QUERY_THRESHOLD=1.0

# External API Keys
SHODAN_API_KEY=your_shodan_api_key_here
CENSYS_API_ID=your_censys_api_id_here
CENSYS_API_SECRET=your_censys_api_secret_here
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Feature Flags
FEATURES__KNOWLEDGE_GRAPH=true
FEATURES__RL_OPTIMIZATION=true
FEATURES__STEALTH_MODE=true
FEATURES__DISTRIBUTED_SCANNING=true
FEATURES__AUTO_EXPLOITATION=false
FEATURES__AI_DECISION_MAKING=true
FEATURES__CONTINUOUS_LEARNING=true
FEATURES__THREAT_INTELLIGENCE=true